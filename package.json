{
  "name": "the-homies-app",
  "version": "1.0.0",
  "description": "A real-time chat application with video calling features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "next dev",
    "build": "echo 'Building app...' && npx prisma generate",
    "postinstall": "npx prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^5.0.0",
    "@supabase/supabase-js": "^2.38.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "megajs": "^1.1.4",
    "next": "^13.4.19",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.7",
    "node-fetch": "^2.6.7",
    "prisma": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9"
  },
  "keywords": [
    "chat",
    "messaging",
    "video",
    "real-time"
  ],
  "license": "MIT"
}
