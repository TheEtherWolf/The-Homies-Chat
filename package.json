{
  "name": "the-homies-app",
  "version": "1.0.0",
  "description": "A real-time chat application with video calling features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "next dev",
    "build": "next build",
    "glitch": "next start -p $PORT",
    "postinstall": "prisma generate && next build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^5.10.2",
    "@supabase/supabase-js": "^2.39.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "megajs": "^1.1.4",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "nodemailer": "^6.9.9",
    "node-fetch": "^2.7.0",
    "prisma": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
